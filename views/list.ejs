
<%- include("header") -%>
<body>

  <div class="box" id="heading">
  <h1><%= listTitle %></h1>
  </div>

  <div class="box">
   <% for(i=0; i<newItemList.length; i++){ %>
    
    <div class="item">
    <input type="checkbox">
      <p><%= newItemList[i] %></p> 
      
  </div>
   <% } %>

  <form class="item" action="/" method="post">

    <input id="inputFieldId" type="text" name="newItem" placeholder="New Item" autocomplete="off">
    <button type="submit" name="list" value= <%= listTitle %>>+</button>

  </form>

  <%- include("footer") -%>
</body>